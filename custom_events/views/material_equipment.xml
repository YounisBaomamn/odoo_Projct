<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="equipment_view_form" model="ir.ui.view">
        <field name="name">equipment.form</field>
        <field name="model">material.equipment</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name_equipment"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
<record id="equipmen_view_tree" model="ir.ui.view">
    <field name="name">equipmen.tree</field>
    <field name="model">material.equipment</field>
    <field name="arch" type="xml">
        <tree>
            <field name="name_equipment"/>
        </tree>
    </field>
</record>
</odoo>
